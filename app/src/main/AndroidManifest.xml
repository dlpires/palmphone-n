<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.appnative.dlpires.palmphone_n">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">


        <!--Pagina de Login (Inicial)-->
        <activity android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>


        <!--Menu Principal-->
        <activity android:name=".MenuPage"></activity>

        <!--Perfil Usuário-->
        <activity android:name=".UserPage"></activity>

        <!--Página de Opções: Coletor-->
        <activity android:name=".ColetorPage"></activity>

        <!--Página de Opções: Coleta de Dados-->
        <activity android:name=".ColetorDadosPage"></activity>

    </application>

</manifest>
